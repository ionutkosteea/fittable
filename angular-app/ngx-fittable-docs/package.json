{
  "name": "ngx-fittable-docs",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --watch",
    "reset": "rm -r dist & rm -r node_modules & rm package-lock.json & npm cache clean --force",
    "link": "npm link fittable-core fittable-model fittable-model-operations fittable-view-model fittable-angular",
    "clean": "rm -r ./dist & rm -r -f ./.angular/cache",
    "build": "ng build  && cp ./LICENSE ./dist/ngx-fittable-docs",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "npx eslint ./src"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.2",
    "@angular/common": "^18.0.2",
    "@angular/compiler": "^18.0.2",
    "@angular/core": "^18.0.2",
    "@angular/forms": "^18.0.2",
    "@angular/platform-browser": "^18.0.2",
    "@angular/platform-browser-dynamic": "^18.0.2",
    "@angular/router": "^18.0.2",
    "fittable-angular": "^5.0.1",
    "npm": "^10.2.5",
    "primeicons": "^6.0.1",
    "primeng": "^17.3.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.3",
    "@angular/cli": "^18.0.3",
    "@angular/compiler-cli": "^18.0.2",
    "@types/jasmine": "~5.1.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "3.2.5",
    "typescript": "~5.4.5"
  }
}